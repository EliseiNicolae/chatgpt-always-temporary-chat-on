{
  "manifest_version": 3,
  "name": "ChatGPT - Keep Temporary Chat ON",
  "version": "1.1",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["toggle.html"],
      "matches": ["https://chatgpt.com/*"]
    }
  ]
}
