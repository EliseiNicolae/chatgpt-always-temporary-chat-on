{
  "manifest_version": 3,
  "name": "ChatGPT: Temporary chat always ON",
  "version": "1.2",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["toggle.html"],
      "matches": ["https://chatgpt.com/*"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
